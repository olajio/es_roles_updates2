{
  "clusters": {
    "prod": {
      "url": "https://prod-elasticsearch:9200",
      "api_key": "YOUR_PROD_API_KEY_HERE",
      "verify_ssl": false,
      "description": "Production Elasticsearch cluster"
    },
    "qa": {
      "url": "https://qa-elasticsearch:9200",
      "api_key": "YOUR_QA_API_KEY_HERE",
      "verify_ssl": false,
      "description": "QA Elasticsearch cluster"
    },
    "dev": {
      "url": "https://dev-elasticsearch:9200",
      "api_key": "YOUR_DEV_API_KEY_HERE",
      "verify_ssl": false,
      "description": "Development Elasticsearch cluster"
    },
    "ccs": {
      "url": "https://ccs-elasticsearch:9200",
      "kibana_url": "https://ccs-kibana:5601",
      "api_key": "YOUR_CCS_API_KEY_HERE",
      "verify_ssl": false,
      "description": "Cross-Cluster Search Elasticsearch cluster"
    }
  },
  "defaults": {
    "remote_inject_patterns": ["partial-*", "restored-*"],
    "ccs_inject_patterns": ["partial-*", "restored-*", "elastic-cloud-logs-*"],
    "ccs_kibana_privileges": [
      "feature_discover.all",
      "feature_dashboard.all",
      "feature_visualize.all"
    ],
    "remote_clusters": ["prod", "qa", "dev"],
    "ccs_cluster": "ccs"
  }
}
